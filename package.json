{
  "private": true,
  "repository": "https://github.com/lblod/ember-mock-login",
  "license": "MIT",
  "author": "redpencil.io",
  "scripts": {
    "build": "pnpm --filter ember-mock-login build",
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter '*' lint:fix",
    "prepare": "pnpm build",
    "start": "concurrently 'pnpm:start:*' --restart-after 5000 --prefixColors auto",
    "start:addon": "pnpm --filter ember-mock-login start --no-watch.clearScreen",
    "start:test-app": "pnpm --filter test-app start",
    "test": "pnpm --filter '*' test",
    "test:ember": "pnpm --filter '*' test:ember",
    "release": "pnpm release --filter ember-mock-login"
  },
  "packageManager": "pnpm@10.5.0",
  "devDependencies": {
    "@glint/core": "^1.2.1",
    "concurrently": "^9.1.2",
    "prettier": "^3.0.3",
    "prettier-plugin-ember-template-tag": "^2.0.2"
  }
}
